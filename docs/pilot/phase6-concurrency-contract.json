{
  "version": "cicsc/phase6-concurrency-contract-v1",
  "supported_model": {
    "name": "stream_serializable_with_scoped_cross_stream_behavior",
    "stream_scope": {
      "identity": [
        "tenantId",
        "entityType",
        "entityId"
      ],
      "guarantees": [
        "append order is monotonic by seq within a stream",
        "replay semantics consume only in-stream events",
        "same-stream write/write conflict resolves as abort-or-serialize"
      ]
    },
    "cross_stream_scope": {
      "guarantees": [
        "no global total order is claimed across distinct streams",
        "interleaving of independent streams is allowed",
        "causality checks are scoped by declared stream identity"
      ],
      "boundaries": [
        "no distributed transaction guarantee",
        "no backend lock-manager equivalence claim",
        "no cross-tenant causality claim"
      ]
    }
  },
  "evidence": {
    "tests": [
      "tests/concurrency/transaction-model.test.ts",
      "tests/concurrency/causality-replay.test.ts",
      "tests/runtime/execute-command-tx-seq.test.ts"
    ],
    "formal": [
      "lean/Cicsc/Core/Semantics/Isolation.lean",
      "lean/Cicsc/Core/Semantics/CausalityReplay.lean"
    ]
  }
}
